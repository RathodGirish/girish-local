<!DOCTYPE html>
<html>
	<link rel="stylesheet" type="text/css" href="css/style.css">
	<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.3.2/jquery.min.js" type="text/javascript"></script>
	<script type="text/javascript">
	$(document).ready(function () {
	    $("#report tr:odd").addClass("odd");
	    $("#report tr:not(.odd)").hide();
	    $("#report tr:first-child").show();

	    $("#report tr.odd").click(function () {
	        $(this).next("tr").toggle();
	        $(this).find(".arrow").toggleClass("up");
	    });
	    $(".hostName").html(window.location.host);

	   	var cnt = 1;
	    $('tr.api-row').each(function() {
		    var currentElement = $(this);
		    $(this).closest('tr').children('td:first').text(cnt);
		    cnt++;
		});

	});
	</script>
	<div class="featured">
		<table class="table table-bordered table-hover">
		   <tr><td></td></tr>
		   <tr><td><b>API HELP</b></td></tr>
		</table>

		<div style="height:10px;"></div>
		<table id="report">
		    <tr style="border-radius: 5px;">
		        <th style="width: 1%;">No</th>
		        <th style="width: 5%;">Method</th>
		        <th style="width: 70%;">Url<b style="float: right; font-family: Verdana; "></b></th>
		        <th style="width: 10%;">Model</th>
		    </tr>
		    
		</table>
	</div>
</html>